<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.RegionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.entity.Region">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="sn" property="sn" />
        <result column="ishot" property="ishot" />
        <result column="seq" property="seq" />
        <result column="info" property="info" />
        <result column="ref_ids" property="refIds" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, sn, ishot, seq, info, ref_ids
    </sql>

</mapper>
